using namespace std;
#include <iostream>

class car
{
private:
	int price;

public:
	car(int p)
	{
		price = p;
	}
	void print();
	friend void increase(car ob);
};

void car::print()
{
	cout << price<<endl;

}
void increase(car ob)
{
	
	ob.price += 1000;
	cout << ob.price;
}
int main()
{
	car c(1900);
	c.print();
	increase(c);

}
