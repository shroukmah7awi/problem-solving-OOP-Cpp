using namespace std;
#include <iostream>


class CompareNumbers
{
private :
	int x;
public:
	void print()
	{
		cout << "Enter x: ";
		cin >> x;
	}
	void compare(CompareNumbers ob1, CompareNumbers ob2)
	{
		if (ob1.x == ob2.x)
		{
			cout << "Equaled";
		}
		else if(ob1.x != ob2.x)
		{
			cout << "Not Equaled";
		}

	}

};



int main()
{

	CompareNumbers ob1;
	CompareNumbers ob2;

	ob1.print();
	ob2.print();

	ob2.compare(ob1,ob2);


}
